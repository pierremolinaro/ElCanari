outletClass PMFontButton : NSButton {
  binding $fontValue : NSFont
    marker nil: "Null Value"
    marker NSNoSelectionMarker: "No Selection"
    marker NSMultipleValuesMarker: "Multiple Values"
    marker NSNotApplicableMarker: "Not Applicable" ;
  exclude $fontValue, $value ;
}

